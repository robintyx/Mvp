<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.microvideo.framework3.project.mapper.CompanyMapper">
    <select id="selectAll" resultType="cn.microvideo.framework3.project.entity.Company">
        select f_int_company_id  AS company_id,
               f_vc_company_name AS company_name
        from t_hw_company
    </select>
</mapper>